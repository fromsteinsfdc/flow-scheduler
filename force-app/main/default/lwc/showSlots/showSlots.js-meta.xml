<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>51.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Display Booking Slots</masterLabel>
    <targets>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="slots" label="Slots" type="apex://BookingSlot[]" role="inputOnly"/>
            <property name="error" label="Error Message" type="String" role="inputOnly"/>
            <property name="startTime" label="Start Time" type="string" role="outputOnly"/>
            <property name="endTime" label="End Time" type="string" role="outputOnly"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
